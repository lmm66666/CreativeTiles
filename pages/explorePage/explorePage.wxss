page{
  height: 100%;
}

swiper{
  width: 100%;
  height: 400rpx;
}

swiper image{
  width: 100%;
  height: 100%;
}

.all{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  background: gainsboro;
}

.iconBar{
  height: 8%;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin: 10rpx;
}

.iconView{
  height: 100%;
  width: 20%;
  display: flex;
  flex-direction: column;
}

.icon{
  height: 70%;
  width: 60%;
  display: flex;
  align-self: center;
}

.iconText{
  height: 15%;
  width: 60%;
  font-size: 11px;
  display: flex;
  align-self: center;
  margin: 3px;
}

.title{
  font-size: 20px;
  margin: 20rpx;
}


.image{
  height: 240rpx;
  width: 100%;
}

.doubleView{
  height: 280rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.theView{
  height: 280rpx;
  width: 45%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 20rpx;
  background: white;
}

.blank{
  height: 30rpx;
  width: 100%;
}